#File Deployment

!{DOUG
deploy:
    "./contracts/SmartVoteDOUG.lll" => {{DOUG}}
!}DOUG

!{PF
modify-deploy:
	"./contracts/PollFactory.lll" => {{PF}}
	(def 'DAPPDOUG 0xdaffe) => (def 'DAPPDOUG {{DOUG}})
!}PF

!{setup
transact:
    {{DOUG}} => "setperm" "doug" {{PF}} 1

transact:
    {{DOUG}} => "setperm" "pollfactory" {{PF}} 1

transact:
    {{DOUG}} => "register" "doug" {{PF}}
!}setup

